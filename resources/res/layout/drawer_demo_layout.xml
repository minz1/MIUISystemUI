<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.android.systemui.fsgesture.WidthAdaptiveView android:id="@+id/drawer_main_img" android:background="@drawable/fsg_demo_drawer_main" android:layout_width="match_parent" android:layout_height="match_parent"/>
    <ImageView android:id="@+id/drawer_img" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/fsg_demo_drawer" android:scaleType="matrix" android:layout_alignParentBottom="true"/>
    <com.android.systemui.fsgesture.FsGestureDemoSwipeView android:id="@+id/fsgesture_swipe_view" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    <View android:id="@+id/shelter_view" android:layout_width="@dimen/fsgesture_shelter_width" android:layout_height="@dimen/fsgesture_shelter_height" android:layout_alignParentTop="true"/>
