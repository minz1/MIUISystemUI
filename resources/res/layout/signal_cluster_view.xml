    </LinearLayout>
    <View android:id="@+id/wifi_signal_spacer" android:visibility="gone" android:layout_width="@dimen/status_bar_wifi_signal_spacer_width" android:layout_height="4dp"/>
</com.android.systemui.statusbar.SignalClusterView>
