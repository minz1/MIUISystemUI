<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#66000000"/>
            <stroke android:width="0.5dp" android:color="#b3000000"/>
            <corners android:radius="3dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="0.5dp" android:color="#b3000000"/>
            <corners android:radius="3dp"/>
        </shape>
    </item>
</selector>
